<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <table>
        <tr>
            <th>id</th>
            <th>name</th>
            <th>gender</th>
            <th>age</th>
        </tr>
    <% for(var i = 0; i < users.length; i++) { %>
        <tr>
            <td><%= users[i].id %></td>
            <td><%= users[i].name %></td>
            <td><%= users[i].gender %></td>
            <td><%= users[i].age %></td>
        </tr>
    <% } %>
    </table>
  </body>
</html>
